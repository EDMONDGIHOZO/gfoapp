{
  "expo": {
    "name": "gfoapp",
    "slug": "gfoapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/images/bg.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#00adef"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],

    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#00adef"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
